# n8n 테스트 환경 설정 예시

# 로컬 개발
N8N_BASE_URL=http://localhost:5678

# 개발 서버
# N8N_BASE_URL=https://dev-n8n.company.com

# 스테이징 서버
# N8N_BASE_URL=https://staging-n8n.company.com

# 프로덕션 서버
# N8N_BASE_URL=https://n8n.company.com

# n8n Cloud
# N8N_BASE_URL=https://myteam.app.n8n.cloud

# 인증이 필요한 경우
# N8N_USERNAME=test@example.com
# N8N_PASSWORD=testpassword

# API 토큰 (API 테스트용)
# N8N_API_TOKEN=your-api-token

# 테스트 설정
TEST_TIMEOUT=60000
HEADLESS=true
BROWSER=chromium